<p class="address" >
    {% for address_field in address_fields %}
        {{ address_field }}<br/>
    {% endfor %}
</p>
<p class="mail-phone">
    <span class="glyphicon glyphicon-envelope" aria-hidden="true"></span>&nbsp;
    <a href="mailto:{{ email }}">{{ email }}</a>
    <br/>
    <span class="glyphicon glyphicon-earphone" aria-hidden="true"></span>&nbsp;
    {{ phone }}
</p>
